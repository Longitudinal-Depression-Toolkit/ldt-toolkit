{
  "standard_machine_learning": {
    "techniques": [
      {
        "id": "run_experiment",
        "name": "Run Experiment",
        "description": "Train and evaluate one standard (cross-sectional) estimator.",
        "parameters": [
          {
            "key": "input_path",
            "label": "Input CSV path",
            "type": "path",
            "required": true,
            "hint": "Tabular dataset in wide format."
          },
          {
            "key": "target_column",
            "label": "Target column",
            "type": "string",
            "required": true,
            "default": "target",
            "hint": "Classification target column."
          },
          {
            "key": "feature_columns",
            "label": "Feature columns",
            "type": "string",
            "required": true,
            "default": "auto",
            "hint": "Comma-separated list or `auto` for all except target."
          },
          {
            "key": "estimator_key",
            "label": "Estimator key",
            "type": "string",
            "required": false,
            "hint": "Optional; defaults to the first discovered estimator."
          },
          {
            "key": "metric_keys",
            "label": "Metric keys",
            "type": "string",
            "required": true,
            "default": "f1_macro",
            "hint": "Comma-separated metrics (e.g. f1_macro, auroc)."
          },
          {
            "key": "cv_folds",
            "label": "Cross-validation folds",
            "type": "int",
            "required": true,
            "default": 10,
            "min": 2
          },
          {
            "key": "validation_split",
            "label": "Validation split",
            "type": "string",
            "required": false,
            "default": "auto",
            "hint": "`auto`, ratio like 80/20, or float in (0,1)."
          },
          {
            "key": "random_seed",
            "label": "Random seed",
            "type": "int",
            "required": true,
            "default": 42
          },
          {
            "key": "output_dir",
            "label": "Output directory",
            "type": "path",
            "required": false,
            "hint": "Defaults to outputs/standard_ml/<estimator_key>."
          },
          {
            "key": "hyperparameters_json",
            "label": "Hyperparameters JSON",
            "type": "json",
            "required": false,
            "default": {},
            "hint": "Estimator constructor overrides as JSON object."
          },
          {
            "key": "multiclass_mode",
            "label": "Multiclass mode",
            "type": "enum",
            "required": true,
            "default": "multiclass",
            "options": [
              {
                "label": "Multiclass",
                "value": "multiclass"
              },
              {
                "label": "One-vs-rest",
                "value": "one_vs_rest"
              }
            ]
          },
          {
            "key": "silent_training_output",
            "label": "Silent training output",
            "type": "bool",
            "required": true,
            "default": true
          },
          {
            "key": "generate_notebook",
            "label": "Generate reproducibility notebook",
            "type": "bool",
            "required": true,
            "default": true
          },
          {
            "key": "notebook_output_path",
            "label": "Notebook output path",
            "type": "path",
            "required": false,
            "hint": "Used only when notebook generation is enabled.",
            "when": {
              "field": "generate_notebook",
              "equals": true
            }
          }
        ]
      },
      {
        "id": "list_estimators",
        "name": "List Estimators",
        "description": "List discoverable standard estimators and metadata.",
        "parameters": []
      },
      {
        "id": "list_metrics",
        "name": "List Metrics",
        "description": "List all supported metrics for standard experiments.",
        "parameters": []
      }
    ]
  },
